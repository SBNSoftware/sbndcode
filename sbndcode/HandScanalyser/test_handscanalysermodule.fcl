BEGIN_PROLOG

handscanalyser:
{ 
  module_type:              "HandScanalyser"
  
  GenieGenModuleLabel:      "generator"
  G4ModuleLabel:            "largeant"
  TrackModuleLabel:         "pandoraTrack"
  ShowerModuleLabel:        "pandoraShower"
  PFParticleModuleLabel:    "pandora"
  VertexModuleLabel:        "pandora"

  ### Parameters to define topological (final state) selections in truth
  TopologyLabels  : ["1mu1p", "1munp", "1munp1cpi", "1mu1p1npi", "1e1p", "1e1p1cpi", "1e1p1npi", "1enpnphnnpi"]
  NumberMuons     : [1,  1,  1,  1,  0,  0,  0,  0]
  NumberElectrons : [0,  0,  0,  0,  1,  1,  1,  1]
  NumberPhotons   : [0,  0,  0,  0,  0,  0,  0, -1]
  NumberProtons   : [1, -1, -1,  1,  1,  1,  1, -1]
  NumberCPions    : [0,  0,  1,  0,  0,  1,  0,  0]
  NumberNPions    : [0,  0,  0,  1,  0,  0,  1, -1]
 
  ## Additional analysis parameters
  OutputFileName : "filtered_file_event_data_numu.csv"
  AllFileName    : "all_file_event_data_numu.csv"
  Verbose        : false
}

END_PROLOG
