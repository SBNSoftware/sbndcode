# job configuration

#include "reco_sbnd.fcl"

process_name: Reco2

physics.reco2_sce: [@sequence::physics.reco2,
            pandoraSCE, pandoraSCETrack, pandoraSCEShower, pandoraSCEShowerSBN, pandoraSCECalo, pandoraSCEPid, crthitt0SCE, crttrackt0SCE, fmatchSCE, fmatchopSCE, fmatcharaSCE, fmatchoparaSCE, opt0finderSCE]

physics.trigger_paths: [ reco2_sce ]

physics.end_paths: [stream1, caloskimana]

