#include "simulationservices_sbnd.fcl"

process_name: PiZeroAnalyzer

services:
{
  TFileService: { fileName: "pizeroana_sbnd.root" }
  @table::sbnd_services
  ParticleInventoryService: @local::sbnd_particleinventoryservice
  BackTrackerService:       @local::sbnd_backtrackerservice
}

source:
{
  module_type: RootInput
}

physics:
{
  analyzers:
  {
    pizeroana:
    { 
      module_type:       "PiZeroAnalysis"
    }
  }

  ana:       [ pizeroana ]
  end_paths: [ ana ]
}
