	<volume name="volInsulation">
		<materialref ref="PU_foam_dense" />
		<solidref ref="boxInsulation"/>
	</volume>
	<volume name="volStructure">
		<materialref ref="STEEL_STRUCTURAL_S460L" />
		<solidref ref="boxStructure_Beams"/>
