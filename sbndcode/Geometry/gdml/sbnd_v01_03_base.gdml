	<position name="secondAPA" unit="mm" x="0" y="0" z="0" />
</define>

<solids>
	<box name="boxAPAFrameBeamLongOuter"  lunit="mm" x="metalon_H"   y="metalon_L1"   z="metalon_W"/>
