    <define>
        <constant name="theCPA_VerticalSpacing" value="1035.0" />
        <constant name="theCPA_HorizontalSpacing" value="1275.0" />
        <constant name="theCPA_LongBar" value="4*theCPA_VerticalSpacing" />
        <constant name="theCPA_ShortBar" value="2*theCPA_HorizontalSpacing" />
        <constant name="theCPA_R" value="26.0" />
        <constant name="theCPA_r" value="25.0" />
        <constant name="theCPA_Y" value="theCPA_LongBar+2*theCPA_R" />
        <constant name="theCPA_Z" value="theTPC_Z" />
        <constant name="theCPA_Half_Z" value="theCPA_ShortBar+2*theCPA_R" />
        
        <constant name="theCPAMesh_W" value="1.0" />
        <constant name="theCPAMesh_Z" value="theCPA_HorizontalSpacing-theCPA_R" />
        <constant name="theCPAMesh_Y" value="theCPA_VerticalSpacing-theCPA_R" />
        
        <position name="posCPA_UStream" unit="mm" x="0" y="0" z="system_DZ-theCPA_HorizontalSpacing-theCPA_R"/>
        <position name="posCPA_DStream" unit="mm" x="0" y="0" z="system_DZ+theCPA_HorizontalSpacing+theCPA_R"/>
        
    </define>
    
    <solids>
        <!-- Field cage units: horizontal (top/bottom) and vertical (upstream/downstream) --> 
        <box  name="boxFieldCage_unit_hor"  lunit="cm" x="4.6"  y="1.1" z="522."/>                                     
        <box  name="boxFieldCage_unit_ver"  lunit="cm" x="4.6"  y="420." z="1.1"/>
        
        <box  name="boxCPAMesh"  lunit="mm" x="theCPAMesh_W"  y="theCPAMesh_Y" z="theCPAMesh_Z"/>
        <!-- reflector foils -->
        <!-- foil thickness = 0.0508 mm -->
        <!-- TPB thickness = 0.000023 mm -->
        <box name="boxCPAFoil" lunit="mm" x="1 + 2*0.0508" y="1009" z="1249"/>
        <box name="boxCPATPB" lunit="mm" x="1 + 2*0.0508 + 2*0.000023" y="1009" z="1249"/>
        
        <box  name="boxCPAPlane"  lunit="mm" x="theCPA_W"  y="theCPA_Y" z="theCPA_Half_Z"/>
        <tube name="tubeLongCPABar_Outter"  rmax="theCPA_R"  z="theCPA_Y"   deltaphi="360"  aunit="deg"  lunit="mm"/>
        <tube name="tubeLongCPABar_Inner"  rmax="theCPA_r"  z="theCPA_Y"   deltaphi="360"  aunit="deg"  lunit="mm"/>
        <tube name="tubeShortCPABar_Outter"  rmax="theCPA_R"  z="theCPA_Half_Z"   deltaphi="360"  aunit="deg"  lunit="mm"/>
        <tube name="tubeShortCPABar_Inner"  rmax="theCPA_r"  z="theCPA_Half_Z"   deltaphi="360"  aunit="deg"  lunit="mm"/>
        
        <subtraction name="tubeLongCPABar">
            <first ref="tubeLongCPABar_Outter"/>
            <second ref="tubeLongCPABar_Inner"/>
            <positionref ref="center"/>
        </subtraction>
        <subtraction name="tubeShortCPABar">
            <first ref="tubeShortCPABar_Outter"/>
            <second ref="tubeShortCPABar_Inner"/>
            <positionref ref="center"/>
        </subtraction>
        
        <union name="assemblingCPA_part_1">
            <first ref="tubeShortCPABar"/>
            <second ref="tubeLongCPABar"/>
            <positionref ref="center"/>
            <rotationref ref="rotateP90X"/>
        </union>
        <union name="assemblingCPA_part_2">
            <first ref="assemblingCPA_part_1"/>
            <second ref="tubeShortCPABar"/>
            <position name="posCPA_Short_1" unit="mm" x="0" y="1*theCPA_VerticalSpacing" z="0"/>
        </union>
        <union name="assemblingCPA_part_3">
            <first ref="assemblingCPA_part_2"/>
            <second ref="tubeShortCPABar"/>
            <position name="posCPA_Short_2" unit="mm" x="0" y="2*theCPA_VerticalSpacing" z="0"/>
        </union>
        <union name="assemblingCPA_part_4">
            <first ref="assemblingCPA_part_3"/>
            <second ref="tubeShortCPABar"/>
            <position name="posCPA_Short_3" unit="mm" x="0" y="(-1)*theCPA_VerticalSpacing" z="0"/>
        </union>
        <union name="assemblingCPA_part_5">
            <first ref="assemblingCPA_part_4"/>
            <second ref="tubeShortCPABar"/>
            <position name="posCPA_Short_5" unit="mm" x="0" y="(-2)*theCPA_VerticalSpacing" z="0"/>
        </union>
        <union name="assemblingCPA_part_6">
            <first ref="assemblingCPA_part_5"/>
            <second ref="tubeLongCPABar"/>
            <position name="posCPA_Short_6" unit="mm" x="0" y="0" z="(-1)*theCPA_HorizontalSpacing"/>
            <rotationref ref="rotateP90X"/>
        </union>
        <union name="assemblingCPA_part_7">
            <first ref="assemblingCPA_part_6"/>
            <second ref="tubeLongCPABar"/>
            <position name="posCPA_Short_7" unit="mm" x="0" y="0" z="1*theCPA_HorizontalSpacing"/>
            <rotationref ref="rotateP90X"/>
        </union>
        
    </solids>
    
    <structure>
        
        <volume name="volCPAMesh">
            <materialref ref="SteelMesh"/>
            <solidref ref="boxCPAMesh"/>
        </volume>
        
        <!-- reflector foils -->
        
        <volume name="volCPAFoil">
            <materialref ref="vm2000"/>
            <solidref ref="boxCPAFoil"/>
            <physvol>
                <volumeref ref="volCPAMesh"/>
                <positionref ref="center"/>
            </physvol>
        </volume>
        
        <volume name="volCPATPB">
            <materialref ref="TPB"/>
            <solidref ref="boxCPATPB"/>
            <physvol>
                <volumeref ref="volCPAFoil"/>
                <positionref ref="center"/>
            </physvol>
        </volume>
        
        
        <volume name="volCPAHorizontalBar">
            <materialref ref="STEEL_STAINLESS_Fe7Cr2Ni"/>
            <solidref ref="tubeShortCPABar"/>
        </volume>
        
        <volume name="volCPAFrame">
            <materialref ref="STEEL_STAINLESS_Fe7Cr2Ni"/>
            <solidref ref="assemblingCPA_part_7"/>
        </volume>
        
        <volume name="volCPA">
            <materialref ref="LAr"/>
            <solidref ref="boxCPAPlane"/>
            <physvol>
                <volumeref ref="volCPAFrame"/>
                <positionref  ref="center"/>
            </physvol>
            
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_1" unit="mm" x="0" y="theCPA_VerticalSpacing/2" z="theCPA_HorizontalSpacing/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_2" unit="mm" x="0" y="theCPA_VerticalSpacing/2+(1)*theCPA_VerticalSpacing" z="theCPA_HorizontalSpacing/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_3" unit="mm" x="0" y="(-1)*theCPA_VerticalSpacing/2" z="theCPA_HorizontalSpacing/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_4" unit="mm" x="0" y="(-1)*theCPA_VerticalSpacing/2+(-1)*theCPA_VerticalSpacing" z="theCPA_HorizontalSpacing/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_5" unit="mm" x="0" y="theCPA_VerticalSpacing/2" z="(-1)*theCPA_HorizontalSpacing/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_6" unit="mm" x="0" y="theCPA_VerticalSpacing/2+(1)*theCPA_VerticalSpacing" z="(-1)*theCPA_HorizontalSpacing/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_7" unit="mm" x="0" y="(-1)*theCPA_VerticalSpacing/2" z="(-1)*theCPA_HorizontalSpacing/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volCPATPB"/>
                <position name="posCPAMesh_8" unit="mm" x="0" y="(-1)*theCPA_VerticalSpacing/2+(-1)*theCPA_VerticalSpacing" z="(-1)*theCPA_HorizontalSpacing/2"/>
            </physvol>
        </volume>
        
    </structure>
    
    <!-- end of CPA -->
    
    
    <!-- Field Cage -->
    <structure>
        <volume name="volFieldCage_unit_hor">                                                                           
            <materialref ref="STEEL_STAINLESS_Fe7Cr2Ni"/>                                                                 
            <solidref ref="boxFieldCage_unit_hor"/>                                                                       
        </volume>                                                                                                       
        <volume name="volFieldCage_unit_ver">                                                                           
            <materialref ref="STEEL_STAINLESS_Fe7Cr2Ni"/>                                                                 
            <solidref ref="boxFieldCage_unit_ver"/>                                                                       
        </volume>                                                                                                       
    </structure>
    
    <!--========================================================================-->
    <!--                Metal Structure for Half an APA Frame                   -->
    <!--========================================================================-->
    <define>
        <!-- local aliases to global variables -->
        <position name="APAbeamHoriz_1" unit="mm" x="0" y="metalon_L1/2-metalon_W/2" z="metalon_L2/2+metalon_W/2" />
        <position name="APAbeamHoriz_2" unit="mm" x="0" y="metalon_L1/2-APAFrame_H" z="metalon_L2/2+metalon_W/2" />
        <position name="APAbeamHoriz_3" unit="mm" x="0" y="-metalon_L1/2+APAFrame_H" z="metalon_L2/2+metalon_W/2" />
        <position name="APAbeamHoriz_4" unit="mm" x="0" y="-metalon_L1/2+metalon_W/2" z="metalon_L2/2+metalon_W/2" />
        
        <position name="APAbeamVerti_1" unit="mm" x="0" y="0" z="metalon_L2+metalon_W" />
        <position name="APAbeamVerti_2" unit="mm" x="0" y="0" z="2*(metalon_L2+metalon_W)" />
        <position name="APAbeamVerti_3" unit="mm" x="0" y="0" z="-(metalon_L2+1.5*metalon_W)" />
        
        <position name="APAJunction_1" unit="mm" x="-(metalon_H+junction_H)/2" y="metalon_L1/2-metalon_W/2" z="(junction_L-metalon_W)/2" />
        <position name="APAJunction_2" unit="mm" x="-(metalon_H+junction_H)/2" y="-metalon_L1/2+APAFrame_H" z="(junction_L-metalon_W)/2" />
        <position name="APAJunction_3" unit="mm" x="-(metalon_H+junction_H)/2" y="metalon_L1/2-metalon_W/2" z="2*metalon_L2+2.5*metalon_W-junction_L/2" />
        <position name="APAJunction_4" unit="mm" x="-(metalon_H+junction_H)/2" y="-(metalon_L1/2-metalon_W/2)" z="2*metalon_L2+2.5*metalon_W-junction_L/2" />
        <position name="APACable" unit="mm" x="-(metalon_H+junction_H)/2" y="0" z="metalon_L2+metalon_W" />
        
        <constant name="APAdistance_planes" value="0" />
        
        <position name="centerAtBorder" unit="mm" x="0" y="0" z="(metalon_W)/2-_eD_/2" />
        
        <position name="firstAPA" unit="mm" x="0" y="0" z="0" />
        <position name="secondAPA" unit="mm" x="0" y="0" z="0" />
    </define>
    
    <solids>
        <box name="boxAPAFrameBeamLongOuter"  lunit="mm" x="metalon_H"   y="metalon_L1"   z="metalon_W"/>
        <box name="boxAPAFrameBeamLongInner"  lunit="mm" x="metalon_H - metalon_T"   y="metalon_L1+_eD_"   z="metalon_W - metalon_T"/>
        <box name="centerBolt" x="_eD_" y="_eD_" z="_eD_" />
        
        <subtraction name="boxAPAFrameBeamLong">
            <first  ref="boxAPAFrameBeamLongOuter"/>
            <second ref="boxAPAFrameBeamLongInner"/>
            <positionref ref="center"/>
        </subtraction>
        
        <box name="boxAPAFrameBeamShortOuter"  lunit="mm" x="metalon_H"   y="metalon_W"   z="metalon_L2+_eD_"/>
        <box name="boxAPAFrameBeamShortInner"  lunit="mm" x="metalon_H - metalon_T"   y="metalon_W - metalon_T"   z="metalon_L2+2*_eD_"/>
        <subtraction name="boxAPAFrameBeamShort">
            <first  ref="boxAPAFrameBeamShortOuter"/>
            <second ref="boxAPAFrameBeamShortInner"/>
            <positionref ref="center"/>
        </subtraction>
        
        <union name="preFrameAPA_1">
            <first  ref="boxAPAFrameBeamLong"/>
            <second ref="boxAPAFrameBeamShort"/>
            <positionref ref="APAbeamHoriz_1"/>
        </union>
        <union name="preFrameAPA_2">
            <first  ref="preFrameAPA_1"/>
            <second ref="boxAPAFrameBeamShort"/>
            <positionref ref="APAbeamHoriz_2"/>
        </union>
        <union name="preFrameAPA_3">
            <first  ref="preFrameAPA_2"/>
            <second ref="boxAPAFrameBeamShort"/>
            <positionref ref="APAbeamHoriz_3"/>
        </union>
        <union name="preFrameAPA_4">
            <first  ref="preFrameAPA_3"/>
            <second ref="boxAPAFrameBeamShort"/>
            <positionref ref="APAbeamHoriz_4"/>
        </union>
        
        <union name="preAPAFrame">
            <first  ref="preFrameAPA_4"/>
            <second ref="preFrameAPA_4"/>
            <positionref ref="APAbeamVerti_1"/>
        </union>
        
        <union name="oneAPAFrameNoJunction">
            <first  ref="preAPAFrame"/>
            <second ref="boxAPAFrameBeamLong"/>
            <positionref ref="APAbeamVerti_2"/>
        </union>
        
        <box name="boxAPAFrameJunctionOuter"  x="junction_H"   y="junction_W"   z="junction_L"  lunit="mm"/>
        <box name="boxAPAFrameJunctionInner"  x="junction_H - junction_T"   y="junction_W - junction_T"   z="junction_L+2*_eD_"  lunit="mm"/>
        <subtraction name="boxAPAFrameJunction">
            <first  ref="boxAPAFrameJunctionOuter"/>
            <second ref="boxAPAFrameJunctionInner"/>
            <positionref ref="center"/>
        </subtraction>
        
        <union name="preFrameAPA_5">
            <first  ref="oneAPAFrameNoJunction"/>
            <second ref="boxAPAFrameJunction"/>
            <positionref ref="APAJunction_1"/>
        </union>
        
        <union name="preFrameAPA_6">
            <first  ref="preFrameAPA_5"/>
            <second ref="boxAPAFrameJunction"/>
            <positionref ref="APAJunction_2"/>
        </union>
        
        <union name="preFrameAPA_7">
            <first  ref="preFrameAPA_6"/>
            <second ref="boxAPAFrameJunction"/>
            <positionref ref="APAJunction_3"/>
        </union>
        
        <union name="preFrameAPA_8">
            <first  ref="preFrameAPA_7"/>
            <second ref="boxAPAFrameJunction"/>
            <positionref ref="APAJunction_4"/>
        </union>
        
        <tube name="diagonalCable"  rmax="cable_R"  z="cable_L"   deltaphi="360"  aunit="deg"  lunit="mm"/>
        
        <union name="preFrameAPA_9">
            <first  ref="preFrameAPA_8"/>
            <second ref="diagonalCable"/>
            <positionref ref="APACable"/>
            <rotationref ref="rotCableAroundX"/>
        </union>
        
        <union name="oneAPAFrame">
            <first ref="centerBolt"/>
            <second  ref="oneAPAFrameNoJunction"/>
            <positionref ref="centerAtBorder"/>
        </union>
    </solids>
    
    <structure> <!-- APA Frame (metal structure) -->
        <volume name="volOneAPAFrame">
            <materialref ref="STEEL_STAINLESS_Fe7Cr2Ni"/>
            <solidref ref="oneAPAFrame"/>
        </volume>
    </structure>
    <!-- end of APA Frame -->
    
    
    <!--========================================================================-->
    <!--                           A Single PMT                                 -->
    <!--========================================================================-->
    
    <solids>
        <!--tube name="PMTVolume" rmax="(4.5*2.54)" z="(11.1*2.54)+0.005" deltaphi="360" aunit="deg" lunit="cm"/-->
        <!-- <tube name="PMT_Underside" rmax="2.54*4.0" z="0.1" deltaphi="360" aunit="deg" lunit="cm"/> -->
        <tube name="PMTVolume" rmax="10.2" z="(11.1*2.54)+0.005" deltaphi="360" aunit="deg" lunit="cm"/>
        <sphere name="PMT_Underside" rmin="9.66" rmax="10.16" deltaphi="360" deltatheta="90" aunit="deg" lunit="cm" />
        <sphere name="PMT_inside" rmin="0" rmax="9.66" deltaphi="360" deltatheta="90" aunit="deg" lunit="cm" />
        <tube name="PMT_Back" rmax="2.54*4.0" z="0.5" deltaphi="360" aunit="deg" lunit="cm"/>
    </solids>
    
    <structure>
        <volume name="volOpDetSensitive">
            <materialref ref="LAr"/> <!-- for some reason, has to be LAr -->
            <solidref ref="PMT_Underside"/>
        </volume>
        
        <volume name="vol_PMT_in">
            <materialref ref="Vacuum"/>
            <solidref ref="PMT_inside"/>
        </volume>
        
        <volume name="vol_PMT_Back">
            <materialref ref="STEEL_STAINLESS_Fe7Cr2Ni"/>
            <solidref ref="PMT_Back"/>
        </volume>
        
        <volume name="volPMT">
            <materialref ref="LAr"/>
            <solidref ref="PMTVolume"/>
            <physvol>
                <volumeref ref="vol_PMT_Back"/>
                <position name="pos_PMT_Back" unit="cm" x="0" y="0" z="(3.0 * 2.54) + 1.25*2.54/2 - (5.5 * 2.54) - 0.5/2+(3.0*2.54)/2 + 1.25*2.54/2 + (2.54*2.5 - 0.1) - 3."/>
            </physvol>
            <physvol>
                <volumeref ref="volOpDetSensitive"/>
                <position name="pos_PMT_Underside" unit="cm" x="0" y="0" z="(7.0 * 2.54)-(5.5 * 2.54) + (2.54*2.5 - 0.1)/2 - 3."/>
            </physvol>
            <physvol>
                <volumeref ref="vol_PMT_in"/>
                <position name="pos_PMT_in" unit="cm" x="0" y="0" z="(7.0 * 2.54)-(5.5 * 2.54) + (2.54*2.5 - 0.1)/2 - 3."/>
            </physvol>
        </volume>
        
    </structure>
    
    <!--========================================================================-->
    <!--                         ARAPUCA                                        -->
    <!--========================================================================-->
    
    <!-- All dimensions used here were extracted from -->
    <!-- https://a360.co/36kgiLB -->
    <!-- https://a360.co/325MXB8 -->
    
    <define>
        
        <!-- External Dimensions -->
        <constant name="lengthArapucaOutX" value="20"/>
        <constant name="lengthArapucaOutY" value="240"/>
        <constant name="lengthArapucaOutZ" value="96"/>
        
        <!-- Filter Dimensions -->
        <constant name="lengthSensitiveWindowX" value="1"/>
        <constant name="lengthSensitiveWindowY" value="98"/>
        <constant name="lengthSensitiveWindowZ" value="75"/>
        
        <!-- Internal Dimensions -->
        <constant name="lengthArapucaInX" value="15"/>
        <constant name="lengthArapucaInY" value="lengthSensitiveWindowY"/>
        <constant name="lengthArapucaInZ" value="lengthSensitiveWindowZ"/>
        
        <!-- Position Details -->
        <constant name="distBetweenArapucasZ" value="56.4"/>
        <constant name="distanceBetweenAcceptanceWindows" value="8"/>
        <constant name="distBetweenPairArapucasZ" value="300"/>
        <constant name="distBetweenPairArapucasY" value="400"/>
        <constant name="AcceptanceWindowDepth" value="1"/>
        
        <!-- Auxiliary Variables -->
        <constant name="ArapucaDisplacement" value="19.85"/>
        <constant name="VolumeExtraDimension" value="5"/>
        <constant name="ModuleExtraDimension" value="5"/>
        <constant name="SmallDisplacement" value="0.05"/>
        
    </define>
    
    <solids>
        
        <box name="ArapucaOut" lunit="mm" x="lengthArapucaOutX" y="lengthArapucaOutY" z="lengthArapucaOutZ"/>
        <box name="ArapucaIn" lunit="mm" x="lengthArapucaInX" y="lengthArapucaInY" z="lengthArapucaInZ"/>
        
        <subtraction name="ArapucaSubCavity1">
            <first ref="ArapucaOut"/>
            <second ref="ArapucaIn"/>
            <position name="posArapucaSub1" unit="mm" x="2+(lengthArapucaOutX-lengthArapucaInX)/2" y="(distanceBetweenAcceptanceWindows+lengthArapucaInY)/2" z="0"/>
        </subtraction>
        <subtraction name="ArapucaSubCavity2">
            <first ref="ArapucaSubCavity1"/>
            <second ref="ArapucaIn"/>
            <position name="posArapucaSub2" unit="mm" x="2+(lengthArapucaOutX-lengthArapucaInX)/2" y="(-1)*(distanceBetweenAcceptanceWindows+lengthArapucaInY)/2" z="0"/>
        </subtraction>
        
        <!-- X-ARAPUCAs -->
        <box name="ArapucaDoubleAcceptanceWindow" lunit="mm" x="lengthSensitiveWindowX" y="(2*lengthSensitiveWindowY+distanceBetweenAcceptanceWindows)" z="lengthSensitiveWindowZ"/>
        
        <subtraction name="ArapucaXExternalWalls">
            <first ref="ArapucaSubCavity2"/>
            <second ref="ArapucaDoubleAcceptanceWindow"/>
            <position name="posSubDoubleWindow" unit="mm" x="(lengthArapucaOutX/2)-(lengthSensitiveWindowX/2)-AcceptanceWindowDepth" y="0" z="0"/>
        </subtraction>
        
        <!-- ARAPUCAs -->
        <box name="ArapucaSingleAcceptanceWindow" lunit="mm" x="lengthSensitiveWindowX" y="lengthSensitiveWindowY" z="lengthSensitiveWindowZ"/>
        
        <!-- Structural Solids -->
        
        <!-- contains a single arapuca: -->
        <box name="ArapucaVolume" lunit="mm" x="lengthArapucaOutX+(2*VolumeExtraDimension)" y="lengthArapucaOutY+(2*VolumeExtraDimension)" z="lengthArapucaOutZ+(2*VolumeExtraDimension)"/>
        
        <!-- contains 8 arapucas for each window: -->
        <box name="ArapucaModule" lunit="mm" x="lengthArapucaOutX+(2*ModuleExtraDimension)" y="(2*distBetweenPairArapucasY)+(lengthArapucaOutY)+(2*ModuleExtraDimension)" z="(2*distBetweenPairArapucasZ)+distBetweenArapucasZ+(2*lengthArapucaOutZ)+(2*ModuleExtraDimension)"/>
        
        <!-- Subtracting the PMTs from the ARAPUCA module to avoid overlap problems -->
        <subtraction name="ArapucaSubModule1">
            <first ref="ArapucaModule"/>
            <second ref="PMTVolume"/>
            <position name="posSub1" unit="mm" x="0" y="DistY" z="-DistZ"/>
            <rotationref ref="rotateN90Y"/>
        </subtraction>
        <subtraction name="ArapucaSubModule2">
            <first ref="ArapucaSubModule1"/>
            <second ref="PMTVolume"/>
            <position name="posSub2" unit="mm" x="0" y="-DistY" z="-DistZ"/>
            <rotationref ref="rotateN90Y"/>
        </subtraction>
        <subtraction name="ArapucaSubModule3">
            <first ref="ArapucaSubModule2"/>
            <second ref="PMTVolume"/>
            <position name="posSub3" unit="mm" x="0" y="DistY" z="DistZ"/>
            <rotationref ref="rotateN90Y"/>
        </subtraction>
        <subtraction name="ArapucaSubModule4">
            <first ref="ArapucaSubModule3"/>
            <second ref="PMTVolume"/>
            <position name="posSub4" unit="mm" x="0" y="-DistY" z="DistZ"/>
            <rotationref ref="rotateN90Y"/>
        </subtraction>
        <subtraction name="ArapucaSubModule5">
            <first ref="ArapucaSubModule4"/>
            <second ref="PMTVolume"/>
            <position name="posSub5" unit="mm" x="0" y="0" z="0"/>
            <rotationref ref="rotateN90Y"/>
        </subtraction>
        
    </solids>
    
    <structure>
        
        <!--photosensitive solids-->
        <volume name="volOpDetSensitive_SingleWindow">
            <materialref ref="LAr"/>
            <solidref ref="ArapucaSingleAcceptanceWindow"/>
        </volume>
        
        <volume name="volOpDetSensitive_DoubleWindow">
            <materialref ref="LAr"/>
            <solidref ref="ArapucaDoubleAcceptanceWindow"/>
        </volume>
        
        <!--arapuca walls-->
        <volume name="volWalls_SingleWindow">
            <materialref ref="G10"/>
            <solidref ref="ArapucaSubCavity2"/>
        </volume>
        
        <volume name="volWalls_DoubleWindow">
            <materialref ref="G10"/>
            <solidref ref="ArapucaXExternalWalls"/>
        </volume>
        
        <!--Building one ARAPUCA -->
        <volume name="volArapuca">
            <materialref ref="LAr"/>
            <solidref ref="ArapucaVolume"/>
            <physvol>
                <volumeref ref="volWalls_SingleWindow"/>
                <position name="posArapuca1" unit="mm" x="0" y="0" z="0"/>
            </physvol>
            <physvol>
                <volumeref ref="volOpDetSensitive_SingleWindow"/>
                <position name="posArapuca2" unit="mm" x="(lengthArapucaOutX-lengthSensitiveWindowX)/2-AcceptanceWindowDepth" y="(lengthSensitiveWindowY+distanceBetweenAcceptanceWindows)/2" z="0"/>
            </physvol>
            <physvol>
                <volumeref ref="volOpDetSensitive_SingleWindow"/>
                <position name="posArapuca3" unit="mm" x="(lengthArapucaOutX-lengthSensitiveWindowX)/2-AcceptanceWindowDepth" y="(-1)*(lengthSensitiveWindowY+distanceBetweenAcceptanceWindows)/2" z="0"/>
            </physvol>
        </volume>
        
        <!--Building one X-ARAPUCA-->
        <volume name="volXArapuca">
            <materialref ref="LAr"/>
            <solidref ref="ArapucaVolume"/>
            <physvol>
                <volumeref ref="volWalls_DoubleWindow"/>
                <position name="posXArapucaWall" unit="mm" x="0" y="0" z="0"/>
            </physvol>
            <physvol>
                <volumeref ref="volOpDetSensitive_DoubleWindow"/>
                <position name="posAddDoubleWindow" unit="mm" x="(lengthArapucaOutX/2)-(lengthSensitiveWindowX/2)-AcceptanceWindowDepth" y="0" z="0"/>
            </physvol>
        </volume>
        
        <!-- 8 X-Arapucas into a module -->
        <volume name="volModuleXArapuca">
            <materialref ref="LAr"/>
            <solidref ref="ArapucaSubModule5"/>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca1" unit="mm" x="0" y="distBetweenPairArapucasY" z="(-1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca2" unit="mm" x="0" y="distBetweenPairArapucasY" z="(+1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca3" unit="mm" x="0" y="0" z="(-1)*(distBetweenPairArapucasZ+((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca4" unit="mm" x="0" y="0" z="(-1)*(distBetweenPairArapucasZ-((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca5" unit="mm" x="0" y="0" z="(+1)*(distBetweenPairArapucasZ+((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca6" unit="mm" x="0" y="0" z="(+1)*(distBetweenPairArapucasZ-((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca7" unit="mm" x="0" y="(-1)*distBetweenPairArapucasY" z="(-1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleArapuca8" unit="mm" x="0" y="(-1)*distBetweenPairArapucasY" z="(+1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
        </volume>
        
        <!-- 7 X-Arapucas and 1 ARAPUCA into a module -->
        <volume name="volModuleArapuca">
            <materialref ref="LAr"/>
            <solidref ref="ArapucaSubModule5"/>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleXArapuca1" unit="mm" x="0" y="distBetweenPairArapucasY" z="(-1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleXArapuca2" unit="mm" x="0" y="distBetweenPairArapucasY" z="(+1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleXArapuca3" unit="mm" x="0" y="0" z="(-1)*(distBetweenPairArapucasZ+((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleXArapuca4" unit="mm" x="0" y="0" z="(-1)*(distBetweenPairArapucasZ-((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volArapuca"/>
                <position name="posModuleXArapuca5" unit="mm" x="0" y="0" z="(+1)*(distBetweenPairArapucasZ+((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleXArapuca6" unit="mm" x="0" y="0" z="(+1)*(distBetweenPairArapucasZ-((distBetweenArapucasZ+lengthArapucaOutZ)/2))"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleXArapuca7" unit="mm" x="0" y="(-1)*distBetweenPairArapucasY" z="(-1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
            <physvol>
                <volumeref ref="volXArapuca"/>
                <position name="posModuleXArapuca8" unit="mm" x="0" y="(-1)*distBetweenPairArapucasY" z="(+1)*(distBetweenArapucasZ+lengthArapucaOutZ)/2"/>
            </physvol>
        </volume>
        
    </structure>
    
    
    
    <!--========================================================================-->
    <!--                 Assembling a Full APA                                  -->
    <!--========================================================================-->
    <define>
        <position name="posHalfAPA_pZ" unit="mm" x="0" y="0" z="(theTPC_Z)/4"/>
        <position name="posHalfAPA_nZ" unit="mm" x="0" y="0" z="-(theTPC_Z)/4"/>
        <position name="posAPAFrame" unit="mm" x="(theAPA_Structure_W-metalon_H)/2" y="0" z="-(theTPC_Z)/4+_eD_/2"/>
    </define>
    
    <solids>
        <box name="boxHalfSideAPA" lunit="mm" x="theAPA_Structure_W" y="theTPC_Y" z="theTPC_Z/2"/>
        <box name="boxFullSideAPA" lunit="mm" x="theAPA_Structure_W" y="theTPC_Y" z="theTPC_Z"/>
    </solids>
    
    <structure> <!-- APA Frame: contains metal structure and Optical Detectors -->
        
        <volume name="volHalfAPA">
            <materialref ref="LAr"/>
            <solidref ref="boxHalfSideAPA"/>
            <physvol>
                <volumeref ref="volOneAPAFrame"/>
                <positionref ref="posAPAFrame"/>
            </physvol>
        </volume>
        
        <volume	name="volOneAPA">
            <materialref ref="LAr" />
            <solidref ref="boxFullSideAPA"/>
            <physvol>
                <volumeref ref="volHalfAPA"/>
                <positionref ref="posHalfAPA_nZ"/>
            </physvol>
            <!--
            <physvol>
            <volumeref ref="volHalfAPA"/>
            <positionref ref="posHalfAPA_pZ"/>
            <scaleref ref="reflection_Z" />
            </physvol>
            -->
        </volume>
    </structure>
    
    <!--========================================================================-->
    <!--                          Wire Planes                                   -->
    <!--========================================================================-->
    <define>
        
        <constant name="visibleWires" value="1" />
        <rotation name="rPlusUVAngleAboutX" unit="deg" x="30" y="0" z="0"/>
        <constant name="commonWire_D" value="visibleWires*0.015" />
        <constant name="commonWire_R" value="0.5*commonWire_D" />
        
        <constant name="planeFrameGap" value="0" />
        <constant name="tpcPLANE_Y" value="theTPC_Y - 2*planeFrameGap" />
        <constant name="tpcPLANE_Z" value="theTPC_Z - 2*planeFrameGap" />
        <constant name="tpcPLANE_Border" value="2*commonWire_R*Sin30"/>
        <constant name="wireMax_Z" value="tpcPLANE_Z-2*tpcPLANE_Border" />
        <constant name="wireMax_Y" value="tpcPLANE_Y-2*tpcPLANE_Border" />
        <constant name="commonWire_L" value="wireMax_Z/Cos30" />
        
        <constant name="TanAlpha" value="wireMax_Y/wireMax_Z"/>
        <constant name="CosAlpha" value="0.784045824462"/>
        <constant name="SinAlpha" value="0.620702944366"/>
        
        <constant name="wirePitch_Z" value="3" />
        <constant name="wirePitch_Y1" value="wirePitch_Z * Tan30" />
        <constant name="wirePitch_Y2" value="wirePitch_Z / Cos30" />
        
        <constant name="originWires_Z"  value="-wireMax_Z/2"/>
        <constant name="originWires_Y1" value="wireMax_Y/2" />
        <constant name="originWires_Y2" value="originWires_Y1 - (wireMax_Z/2 * Tan30) + 1.2" />
        <constant name="originWires_Y3" value="(wireMax_Z - wireMax_Y)*Tan30/2" />
        <constant name="originWires_Z3" value="-tpcPLANE_Z/2 + tpcPLANE_Border - 1.2" />
        
        <constant name="wireScaleFactor" value="2*wirePitch_Z / wireMax_Z" />
        <constant name="wireUnit_L" value="wireScaleFactor * commonWire_L" />
        
        <!--fixes to make pitches uniform     -->
        <constant name="dp1" value="3.-2.95327" />
        <constant name="dp2" value="3.12786-3" />
        
        <variable name="iWire_1" value="0"/>
        <variable name="iWire_2" value="0"/>
        <variable name="iWire_Y" value="0"/>
        <variable name="iWire_3" value="0"/>
        <variable name="iWire_4" value="833"/>
        <variable name="iWire_5" value="832"/>
        <variable name="iWire_6" value="0"/>
        <variable name="iWire_7" value="1153"/>
        <variable name="iWire_8" value="832"/>
        
    </define>
    
    <solids>
        <box  name="boxTPCPlane"  lunit="mm" x="theTPCPlane_W"  y="tpcPLANE_Y" z="tpcPLANE_Z"/>
        <tube name="tubeWire" lunit="mm" rmax="commonWire_R" z="commonWire_L" aunit="deg" deltaphi="360." />
        <tube name="tubeWireVert" lunit="mm" rmax="commonWire_R" z="wireMax_Y" aunit="deg" deltaphi="360." />
        
        <!-- starts from 0 -->
        <loop for ="iWire_1" to="832" step="1"> <!-- #wire -->
            <tube name="tubeWire_iWire_1" lunit="mm" rmax="commonWire_R" z="(1+iWire_1)*wireUnit_L-4*0.04" aunit="deg" deltaphi="360." />
        </loop>
    </solids>
    
    <structure>
        <volume name="volTPCWire">
            <materialref ref="Copper_Beryllium_alloy25"/>
            <solidref ref="tubeWire"/>
        </volume>
        
        <volume name="volTPCWireVert">
            <materialref ref="Copper_Beryllium_alloy25"/>
            <solidref ref="tubeWireVert"/>
        </volume>
        
        <!-- starts from 0 -->
        <loop for ="iWire_2" to="832" step="1"> <!-- #wire -->
            <volume name="volTPCWire_iWire_2">
                <materialref ref="Copper_Beryllium_alloy25"/>
                <solidref ref="tubeWire_iWire_2"/>
            </volume>
        </loop>
    </structure>
    
    <structure>
        <volume name="volTPCPlaneVert">
            <materialref ref="LAr"/>
            <solidref ref="boxTPCPlane"/>
            
            <!-- Place Y Wires here -->
            <!-- starts from 0 -->
            <loop for ="iWire_Y" to="1665" step="1"> <!-- #wire -->
                <physvol>
                    <volumeref ref="volTPCWireVert"/>
                    <rotationref ref="rotateN90X"/>
                    <position  name="posWire_Y_iWire_Y" unit="mm" x="0" y="0" z="(1+iWire_Y)*wirePitch_Z+originWires_Z"/>
                </physvol>
            </loop>
            
        </volume>
    </structure>
    
    <structure>
        <volume name="volTPCPlane_U">
            <materialref ref="LAr"/>
            <solidref ref="boxTPCPlane"/>
            
            <!-- Place U Wires here -->
            <!-- starts from 0 -->
            <loop for ="iWire_3" to="832" step="1"> <!-- #wire -->
                <physvol>
                    <volumeref ref="volTPCWire_iWire_3"/>
                    <rotationref ref="rotateP30X"/>
                    <position  name="posWire_U1_iWire_3" unit="mm" x="0" y="-(1+iWire_3)*wirePitch_Y1+originWires_Y1+dp1*Cos30" z="(1+iWire_3)*wirePitch_Z+originWires_Z-dp1*Sin30"/>
                </physvol>
            </loop>
            
            <!-- starts from 833 -->
            <loop for ="iWire_4" to="1153" step="1"> <!-- #wire -->
                <physvol>
                    <volumeref ref="volTPCWire"/>
                    <rotationref ref="rotateP30X"/>
                    <position  name="posWire_U2_iWire_4" unit="mm" x="0" y="-(1+iWire_4-833)*wirePitch_Y2+originWires_Y2" z="0"/>
                </physvol>
            </loop> <!-- this line was commented out -->
            
            <!-- starts from 832 -->
            <loop for ="iWire_5" to="1" step="-1">	<!-- #wire -->
                <physvol>
                    <volumeref ref="volTPCWire_iWire_5"/>
                    <rotationref ref="rotateP30X"/>
                    <position  name="posWire_U3_iWire_5" unit="mm" x="0" y="(iWire_5+1)*wirePitch_Y1-originWires_Y1+dp2*Cos30" z="-(iWire_5+1)*wirePitch_Z-originWires_Z-dp2*Sin30"/>
                </physvol>
            </loop>
        </volume>
        
        <volume name="volTPCPlane_V">
            <materialref ref="LAr"/>
            <solidref ref="boxTPCPlane"/>
            
            <!-- Place V Wires here -->
            <!-- starts from 0 -->
            <loop for ="iWire_6" to="832" step="1"> <!-- #wire -->
                <physvol>
                    <volumeref ref="volTPCWire_iWire_6"/>
                    <rotationref ref="rotateN30X"/>
                    <position  name="posWire_V3_iWire_6" unit="mm" x="0" y="(iWire_6+1)*wirePitch_Y1-originWires_Y1+dp2*Cos30" z="(iWire_6+1)*wirePitch_Z+originWires_Z+dp2*Sin30"/>
                </physvol>
            </loop>
            
            <!-- starts from 1153 -->
            <loop for ="iWire_7" to="833" step="-1"> <!-- #wire -->
                <physvol>
                    <volumeref ref="volTPCWire"/>
                    <rotationref ref="rotateN30X"/>
                    <position  name="posWire_V2_iWire_7" unit="mm" x="0" y="-(1+iWire_7-833)*wirePitch_Y2+originWires_Y2" z="0"/>
                </physvol>
            </loop>
            
            <!-- starts from 832 -->
            <loop for ="iWire_8" to="1" step="-1">	<!-- #wire -->
                <physvol>
                    <volumeref ref="volTPCWire_iWire_8"/>
                    <rotationref ref="rotateN30X"/>
                    <position  name="posWire_V1_iWire_8" unit="mm" x="0" y="originWires_Y1-(1+iWire_8)*wirePitch_Y1+dp1*Cos30" z="-originWires_Z-(1+iWire_8)*wirePitch_Z+dp1*Sin30"/>
                </physvol>
            </loop>
            
            
            
        </volume>
        
        
    </structure>
    
    
    <!--========================================================================-->
    <!--                 The Detector                                           -->
    <!--========================================================================-->
    <define>
        <rotation name="rPlus45AboutX" unit="deg" x="45" y="0" z="0"/>
        <rotation name="rPlus90AboutX" unit="deg" x="90" y="0" z="0"/>
        <rotation name="rPlus180AboutY" unit="deg" x="0" y="180" z="0"/>
        
        <position name="posTPCatCryo_p" unit="mm" x="(theTPC_X+theCPA_W)/2" y="0" z="system_DZ"/>
        <position name="posTPCatCryo_n" unit="mm" x="-(theTPC_X+theCPA_W)/2" y="0" z="system_DZ"/>
        <position name="posTPC" unit="mm" x="0" y="0" z="theTPC_Z/2"/>
        
        <position name="posTPCPlane_pU" unit="mm" x="(theTPC_X-5*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posTPCPlane_pV" unit="mm" x="(theTPC_X-3*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posTPCPlane_pY" unit="mm" x="(theTPC_X-1*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posTPCActive_p" unit="mm" x="(theTPC_X-theTPCActive_W-6*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posAPAFrame_pX" unit="mm" x="(theTPC_X+theAPA_Structure_W/2+theCPA_W/2)" y="0" z="0" />
        
        <position name="posAPA_p" 	unit="mm" x="(theTPC_X+theAPA_Structure_W)/2" y="0" z="theTPC_Z/2"/>
        
        <position name="posTPCPlane_nU" unit="mm" x="-(theTPC_X-5*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posTPCPlane_nV" unit="mm" x="-(theTPC_X-3*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posTPCPlane_nY" unit="mm" x="-(theTPC_X-1*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posTPCActive_n" unit="mm" x="-(theTPC_X-theTPCActive_W-6*theTPCPlane_W)/2" y="0" z="0"/>
        <position name="posAPAFrame_nX" unit="mm" x="-(theTPC_X+theAPA_Structure_W/2+theCPA_W/2)" y="0" z="0" />
    </define>
    
    <define>
        <constant name="pmt_DZ" value="275" />
        <constant name="pmt_DY" value="275" />
        <constant name="pmtSet_DZ" value="(theTPC_Z)/8"/>
        <constant name="pmtSet_DY" value="(theTPC_Y)/3"/>
        <constant name="pmt_X" value="theTPC_X+180" />
        <constant name="frame_DZ" value="(theTPC_Z)/4" />
        
        <position name="posAPAFrame_nXnZ" unit="mm" x="-(theTPC_X+metalon_H/2+theCPA_W/2)" y="0" z="-(theTPC_Z)/2+system_DZ" />
        <position name="posAPAFrame_nXpZ" unit="mm" x="-(theTPC_X+metalon_H/2+theCPA_W/2)" y="0" z="system_DZ+0.0045" />
        <position name="posAPAFrame_pXnZ" unit="mm" x="(theTPC_X+metalon_H/2+theCPA_W/2)" y="0" z="-(theTPC_Z)/2+system_DZ" />
        <position name="posAPAFrame_pXpZ" unit="mm" x="(theTPC_X+metalon_H/2+theCPA_W/2)" y="0" z="system_DZ+0.0045" />
        <!-- I am adding 0.0045mm to fix an APA overlap between the two frames-->
    </define>
    
    
    <solids>
        <box name="boxTPC" lunit="mm" x="theTPC_X" y="theTPC_Y" z="theTPC_Z"/>
        <box name="boxTPCActive" lunit="mm" x="theTPCActive_W" y="theTPC_Y" z="theTPC_Z"/>
    </solids>
    
    <structure> <!-- a TPC with 1 active volume and 3 planes each -->
        <volume name="volTPCActive">
            <materialref ref="LAr"/>
            <solidref ref="boxTPCActive"/>
        </volume>
        
        <volume name="volTPC_N"> <!-- a TPC at Negative X -->
            <materialref ref="LAr"/>
            <solidref ref="boxTPC"/>
            <physvol>
                <volumeref ref="volTPCActive"/>
                <positionref ref="posTPCActive_n"/>
            </physvol>
            <physvol>
                <volumeref ref="volTPCPlane_V"/>
                <positionref ref="posTPCPlane_nV"/>
            </physvol>
            <physvol>
                <volumeref ref="volTPCPlane_U"/>
                <positionref ref="posTPCPlane_nU"/>
            </physvol>
            <physvol>
                <volumeref ref="volTPCPlaneVert"/>
                <positionref ref="posTPCPlane_nY"/>
            </physvol>
        </volume>
        
        <volume name="volTPC_P"> <!-- a TPC at Positive X -->
            <materialref ref="LAr"/>
            <solidref ref="boxTPC"/>
            <physvol>
                <volumeref ref="volTPCActive"/>
                <positionref ref="posTPCActive_p"/>
            </physvol>
            <physvol>
                <volumeref ref="volTPCPlane_V"/>
                <positionref ref="posTPCPlane_pU"/>
            </physvol>
            <physvol>
                <volumeref ref="volTPCPlane_U"/>
                <positionref ref="posTPCPlane_pV"/>
            </physvol>
            <physvol>
                <volumeref ref="volTPCPlaneVert"/>
                <positionref ref="posTPCPlane_pY"/>
            </physvol>
        </volume>
    </structure>
    
    <solids>
        <box name="boxIPE450Beam_V" lunit="mm" x="IPE_Beam_T" y="IPE_Beam_L" z="IPE_Beam_W"/>
        <box name="boxIPE450Beam_H1" lunit="mm" x="IPE_Beam_T" y="structure_Z+2*structure_W" z="IPE_Beam_W"/>
        <box name="boxIPE450Beam_H2" lunit="mm" x="IPE_Beam_T+_eD_" y="IPE_Beam_W" z="structure_X+2*structure_W"/>
        <box name="boxStructureOuter" lunit="mm" x="structure_X+structure_W" y="structure_Y+structure_W" z="structure_Z+structure_W"/>
        <box name="boxStructureInner" lunit="mm" x="structure_X" y="structure_Y" z="structure_Z"/>
        <subtraction name="boxStructure_NoBeams">
            <first ref="boxStructureOuter"/>
            <second ref="boxStructureInner"/>
            <positionref ref="center"/>
        </subtraction>
        <union name="boxStructure_1_Beams">
            <first ref="boxStructure_NoBeams"/>
            <second ref="boxIPE450Beam_V"/>
            <position name="posIPEBeam_1" unit="mm" x="IPE_Beam_DX" y="IPE_Beam_DY" z="-((structure_Z+IPE_Beam_W)/2+structure_W)" />
        </union>
        <union name="boxStructure_2_Beams">
            <first ref="boxStructure_1_Beams"/>
            <second ref="boxIPE450Beam_V"/>
            <position name="posIPEBeam_2" unit="mm" x="-IPE_Beam_DX" y="IPE_Beam_DY" z="-((structure_Z+IPE_Beam_W)/2+structure_W)" />
        </union>
        <union name="boxStructure_3_Beams">
            <first ref="boxStructure_2_Beams"/>
            <second ref="boxIPE450Beam_V"/>
            <position name="posIPEBeam_3" unit="mm" x="IPE_Beam_DX" y="IPE_Beam_DY" z="((structure_Z+IPE_Beam_W)/2+structure_W)" />
        </union>
        <union name="boxStructure_4_Beams">
            <first ref="boxStructure_3_Beams"/>
            <second ref="boxIPE450Beam_V"/>
            <position name="posIPEBeam_4" unit="mm" x="-IPE_Beam_DX" y="IPE_Beam_DY" z="((structure_Z+IPE_Beam_W)/2+structure_W)" />
        </union>
        <union name="boxStructure_5_Beams">
            <first ref="boxStructure_4_Beams"/>
            <second ref="boxIPE450Beam_H1"/>
            <position name="posIPEBeam_5" unit="mm" x="-IPE_Beam_DX" y="-((structure_Y+IPE_Beam_W)/2+structure_W)" z="0" />
            <rotationref ref="rotateN90X"/>
        </union>
        <union name="boxStructure_6_Beams">
            <first ref="boxStructure_5_Beams"/>
            <second ref="boxIPE450Beam_H1"/>
            <position name="posIPEBeam_6" unit="mm" x="IPE_Beam_DX" y="-((structure_Y+IPE_Beam_W)/2+structure_W)" z="0" />
            <rotationref ref="rotateN90X"/>
        </union>
        <union name="boxStructure_7_Beams">
            <first ref="boxStructure_6_Beams"/>
            <second ref="boxIPE450Beam_V"/>
            <position name="posIPEBeam_7" unit="mm" x="-((structure_X+IPE_Beam_W)/2+structure_W)" y="IPE_Beam_DY" z="-IPE_Beam_DZ*3" />
            <rotationref ref="rotateN90Y"/>
