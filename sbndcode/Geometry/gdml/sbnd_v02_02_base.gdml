        <union name="build4">
            <first  ref="build3"/>
            <second ref="boxVerticalSlab"/>
            <position name="posVS4" unit="mm" x="-((dHX+s_W)/2+concW)" y="-_eD_" z="-(dHZ+concW)/6"/>

