            <second ref="boxPumpHall"/>
            <position name="posPumpHall" unit="mm" x="(dHX+pumpHall_X)/2-_eD_" y="-(dHY-pumpHall_Y)/2" z="(dHZ+concW)/3"/>

        </subtraction>

        <union name="build0">
            <first  ref="detectorPumpHallWalls"/>
            <second ref="boxFoundationSlab"/>
