					<rotationref ref="rotateP30X"/>
					<position  name="posWire_V_iWire_V2" unit="mm" x="0" y="-0.5*dimWirePitch_Y_mm-(iWire_V2-channels_Middle-(channels_Higher-channels_Middle)/2.0)*dimWirePitch_Y_mm" z="0"/>
				</physvol>
