			<materialref ref="LAr"/>
			<solidref ref="XArapucaVolume"/>
			<auxiliary auxtype="SensDet" auxvalue="SimEnergyDeposit"/>
			<auxiliary auxtype="StepLimit" auxvalue="0.01" unit="mm"/>
			<auxiliary auxtype="Efield" auxvalue="0."/>
			<physvol>
