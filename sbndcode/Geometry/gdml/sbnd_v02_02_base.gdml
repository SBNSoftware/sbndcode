        <union name="build2">
            <first  ref="build1"/>
            <second ref="boxVerticalSlab"/>
            <position name="posVS2" unit="mm" x="(dHX+s_W)/2+concW" y="-_eD_" z="-(dHZ+concW)/6"/>

